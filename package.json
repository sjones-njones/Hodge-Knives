{
  "name": "hodge-knives",
  "version": "1.0.0",
  "description": "A site for viewing and selling custom-made knives.",
  "main": "index.js",
  "scripts": {
    "install": "cd client && npm install && cd ../server && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && npm run start",
    "dev": "concurrently -n client,server \"cd client && npm run dev\" \"cd server && npm run dev\"",
    "lint": "eslint .",
    "seed": "node ./server/config/seeds.js",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.14.0",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.1.0",
    "dotenv": "^16.3.1",
    "eslint-config-wesbos": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "nodemailer": "^6.9.9",
    "react-bootstrap": "^2.9.0-beta.1",
    "react-confetti": "^6.1.0",
    "react-grid-layout": "^1.4.4",
    "react-router-dom": "^6.20.1"
  }
}
